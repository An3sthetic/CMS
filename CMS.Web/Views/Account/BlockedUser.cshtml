@model IEnumerable<CMS.Web.Data.ApplicationUser>

@{
    ViewData["Title"] = "BlockedUser";
}

<h2>BlockedUser</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-action="UnblockUser" asp-route-id="@item.Id">Unblock</a>
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.ProfileImage)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.FbID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.TwitterID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.LinkedinID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.InstagramID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.YoutubeID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.GithubID)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.NormalizedUserName)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.NormalizedEmail)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.EmailConfirmed)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.PasswordHash)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.SecurityStamp)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.ConcurrencyStamp)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.PhoneNumberConfirmed)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.TwoFactorEnabled)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.LockoutEnd)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.LockoutEnabled)
            </td>
            <td>
                @Html.HiddenFor(modelItem => item.AccessFailedCount)
            </td>

        </tr>
}
    </tbody>
</table>
